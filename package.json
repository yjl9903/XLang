{
  "name": "@xlor/xlang",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yjl9903/XLang.git"
  },
  "license": "MIT",
  "author": "XLor",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "bin": "xlang.mjs",
  "files": [
    "dist",
    "*.mjs"
  ],
  "scripts": {
    "build": "unbuild",
    "docs:build": "vuepress build docs",
    "docs:dev": "vuepress dev docs",
    "format": "prettier --write src/**/*.ts test/**/*.ts",
    "release": "bumpp --commit --push --tag && pnpm publish --access public",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "preversion": "unbuild"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --parser=typescript --write",
      "git add"
    ]
  },
  "dependencies": {
    "@xlor/xlex": "^1.0.0",
    "@xlor/xparse": "^1.0.0",
    "cac": "^6.7.12"
  },
  "devDependencies": {
    "@types/node": "17.0.43",
    "bumpp": "8.2.1",
    "husky": "4.3.8",
    "lint-staged": "12.5.0",
    "prettier": "2.7.1",
    "pseudocode": "2.2.0",
    "typescript": "4.7.4",
    "unbuild": "0.7.4",
    "vitest": "0.15.1",
    "vuepress": "1.9.7",
    "vuepress-plugin-mathjax": "1.2.8"
  },
  "packageManager": "pnpm@7.2.1"
}
